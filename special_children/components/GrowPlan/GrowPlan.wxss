.tit {
  line-height: 100rpx;
  font-size: 38rpx;
  font-weight: bold;
  color: #444;
}
scroll-view {
  display: flex;
}
.view_item {
  margin: 14rpx;
}
scroll-view image {
  width: 215rpx;
  height: 215rpx;
  border-radius: 20rpx;
  -webkit-border-radius: 20rpx;
  -moz-border-radius: 20rpx;
  -ms-border-radius: 20rpx;
  -o-border-radius: 20rpx;
}
.des {
  width: 215rpx;
  text-align: center;
  font-size: 28rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
.notice {
  text-align: center;
}
.summary {
  font-weight: bold;
}
.speed_wrap {
  display: flex;
  align-items: center;
}
.speed {
  position: relative;
  width: 145rpx;
  height: 14rpx;
  margin: 10rpx 0;
  border-radius: 10rpx;
  background-color: #cfc3c3;
}
.speed_inner {
  position: absolute;
  border-left: none;
  height: 14rpx;
  left: 0;
  border-radius: 10rpx;
}
.ratio {
  flex: 1;
  text-align: center;
  font-size: 20rpx;
  color: black;
}
